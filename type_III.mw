<Group labelreference="L10" drawlabel="true">
<Input>
<Text-field style="Text" layout="Normal">new formula for pmf of type III runs</Text-field>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">gnk := proc(n,k,x, p)
  local i, j, s, t, st:

  st := time():

  s := 0:
  for i from 0 to n-k do
    for j from max(0, x-2, i-1) to min(n-k-k*i-1, x+i) do
      t := (i+1) * (i*x-i + 2*j+2) 
            * (j)! / (j-x+2)! / (j-i+1)! / (x+i-j)! 
            * binomial(n-k-j-k*i+i,i+1) * (p)^(-i+j+1+k*i) *(1-p)^i
            * (-1)^(j-x):
      s := s + t:
    od:
  od:
  s := s*p^k:

  if (x=0) then 
    s := s + 1 - (n-k+1)*p^k:
  fi:

  if (x=1) then 
    s := s + (n-k+1)*p^k:
  fi:
  print(factor(s)):
  print(`Time: `, time()-st):
end proc;</Text-field>
